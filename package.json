{
  "name": "json-to-sql-flattener",
  "version": "2.0.0",
  "description": "Production-ready JSON to SQL data transformation tool with comprehensive TDD/BDD testing",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:bdd && npm run test:e2e",
    "test:all:coverage": "npm run test:unit:coverage && npm run test:integration:coverage && npm run test:bdd && npm run test:e2e",
    
    "test:unit": "jest --testMatch='**/__tests__/unit/**/*.test.{js,jsx}'",
    "test:unit:coverage": "jest --testMatch='**/__tests__/unit/**/*.test.{js,jsx}' --coverage",
    "test:unit:watch": "jest --testMatch='**/__tests__/unit/**/*.test.{js,jsx}' --watch",
    
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.js' --runInBand",
    "test:integration:coverage": "jest --testMatch='**/__tests__/integration/**/*.test.js' --coverage --runInBand",
    
    "test:bdd": "cucumber-js",
    "test:bdd:watch": "nodemon --exec 'cucumber-js'",
    
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    
    "test:performance": "artillery run __tests__/performance/load-test.yml",
    "test:security": "npm audit && snyk test",
    "test:accessibility": "jest --testMatch='**/__tests__/accessibility/**/*.test.{js,jsx}'",
    
    "coverage:report": "nyc report --reporter=html --reporter=text",
    "coverage:check": "nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100",
    "coverage:upload": "codecov",
    
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    
    "prepare": "husky install",
    "precommit": "lint-staged && npm run test:unit",
    "prepush": "npm run test:unit && npm run test:integration"
  },
  "keywords": [
    "json",
    "sql",
    "elasticsearch",
    "mysql",
    "etl",
    "data-transformation",
    "tdd",
    "bdd"
  ],
  "author": "Mac",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mysql2": "^3.6.5",
    "@elastic/elasticsearch": "^8.11.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@axe-core/react": "^4.8.2",
    "jest-axe": "^8.0.0",
    
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "babel-jest": "^29.7.0",
    
    "supertest": "^6.3.3",
    "nock": "^13.4.0",
    "sinon": "^17.0.1",
    "faker": "^6.6.6",
    "@faker-js/faker": "^8.3.1",
    
    "@cucumber/cucumber": "^10.0.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    
    "@playwright/test": "^1.40.1",
    "playwright": "^1.40.1",
    
    "artillery": "^2.0.3",
    "artillery-plugin-metrics-by-endpoint": "^1.4.0",
    
    "nyc": "^15.1.0",
    "codecov": "^3.8.3",
    
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.1.1",
    
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    
    "mysql": "^2.18.1",
    "testcontainers": "^10.4.0",
    
    "snyk": "^1.1276.0",
    "npm-audit-resolver": "^3.0.0-RC.0"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "backend/**/*.js"
    ],
    "exclude": [
      "**/__tests__/**",
      "**/node_modules/**",
      "**/coverage/**"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
